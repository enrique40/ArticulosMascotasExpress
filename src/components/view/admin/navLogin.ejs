<script>
  const carousel = document.querySelector("#carouselExampleSlidesOnly");

  function changeImage() {
    const activeSlide = carousel.querySelector(".active");
    const nextSlide = activeSlide === carousel.querySelector(".carousel-item:last-child") ? carousel.querySelector(".carousel-item:first-child") : activeSlide.nextElementSibling;

    activeSlide.classList.remove("active");
    nextSlide.classList.add("active");
  }

  setInterval(changeImage, 5000);
</script>